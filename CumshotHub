local blocktptab = Window:CreateTab("Block tp thing yeah ", nil) 
blocktptab:CreateSection("tp w bag")
local oreList = game:GetService("ReplicatedStorage").Content.Ores
local oreOptions = {}
for _,oreModel in oreList:GetChildren() do
    table.insert(oreOptions,oreModel.Name)
end
local matbtp = blocktptab:CreateDropdown({
    Name = "Material Whitelist",
    Options = oreOptions,
    CurrentOption = {},
    MultipleOptions = true,
    Callback = function()end
})

local trybring = blocktptab:CreateButton({
    Name = "try tp mat thing",
    Callback = function()
        local rem = game:GetService("Players").LocalPlayer:WaitForChild("Action")
        local char = plr.Character
        local oldPos = char.CFrane
        if not char:FindFirstChild("Item Bag") then
            plr.Backpack["Item Bag"].Parent = char
        end
        for _,v in workspace.Grab:GetChildren() do
            if v:FindFirstChild("Owner") and v.Owner.Value == plr and v:GetAttribute("Material") == "Stone" then
                char:PivotTo(v.Part.CFrame)
                task.wait(.1)
                rem:FireServer("Store",v.Part)
                break
            end
        end
        task.wait(.1)
        rem:FireServer("Drop")
    end
})

local visualsTab = Window:CreateTab("esp stuff", nil)
visualsTab:CreateSection("Ore ESP")
local worldOres = workspace.WorldSpawn.Ores

table.sort(oreOptions)

local oreESPDropdown = visualsTab:CreateDropdown({
    Name = "Ore Whitelist",
    Options = oreOptions,
    CurrentOption = {},
    MultipleOptions = true,
    Callback = function()end
})

local ESPTextSize = visualsTab:CreateInput({
    Name = "Text Size",
    CurrentValue = "1",
    PlaceholderText = "",
    Callback = function()end
})


local oreESPToggle
oreESPToggle = visualsTab:CreateToggle({
    Name = "Enable ESP",
    CurrentValue = false,
    Callback = function(val)
        if not val then return end
        
        while oreESPToggle.CurrentValue do
            local root = plr.Character and plr.Character:FindFirstChild("HumanoidRootPart")
            local selected = oreESPDropdown.CurrentOption
            
            if root then
                for _, ore in ipairs(worldOres:GetChildren()) do
                    if table.find(selected, ore.Name) then
                        -- Determine the best part to attach ESP to
                        local hittableFolder = ore:FindFirstChild("Hittable")
                        local mainPart = ore:FindFirstChild("Hitbox") or ore.PrimaryPart or ore:FindFirstChildWhichIsA("BasePart")
                        
                        -- 1. Handle Box Adornments (Hittables)
                        if hittableFolder then
                            for _, hb in ipairs(hittableFolder:GetChildren()) do
                                if hb:IsA("BasePart") and not hb:FindFirstChild("OreESP") then
                                    local box = Instance.new("BoxHandleAdornment")
                                    box.Name, box.Adornee, box.AlwaysOnTop, box.ZIndex = "OreESP", hb, true, 10
                                    box.Size, box.Transparency, box.Color3 = hb.Size + Vector3.new(0.1, 0.1, 0.1), 0.5, Color3.new(1, 0, 0)
                                    box.Parent = hb
                                end
                            end
                        end

                        -- 2. Handle Billboard Text (Fallback to mainPart if Hitbox is missing)
                        if mainPart then
                            local dist = math.floor((root.Position - mainPart.Position).Magnitude)
                            local espText = mainPart:FindFirstChild("ESPText")
                            
                            if not espText then
                                local gui = Instance.new("BillboardGui")
                                gui.Name, gui.Adornee, gui.AlwaysOnTop, gui.Size = "ESPText", mainPart, true, UDim2.fromScale(3, 9)
                                gui.StudsOffset = Vector3.new(0, 2, 0)
                                
                                local label = Instance.new("TextLabel")
                                label.Size, label.BackgroundTransparency, label.TextScaled = UDim2.fromScale(1, 1), 1, true
                                label.TextColor3, label.TextStrokeTransparency = Color3.new(1, 1, 1), 0
                                label.Parent = gui
                                gui.Parent = mainPart
                                espText = gui
                            end
                            espText.TextLabel.Text = string.format("%s | %d studs", ore.Name, dist)
                            local size = tonumber(ESPTextSize.CurrentValue) or 1
                            espText.Size = UDim2.fromScale(1000, size)
                        end
                    end
                end
            end
            task.wait(0.1)
        end
        for _, ore in ipairs(worldOres:GetChildren()) do
            for _, desc in ipairs(ore:GetDescendants()) do
                if desc.Name == "OreESP" or desc.Name == "ESPText" then
                    desc:Destroy()
                end
            end
        end
    end
})
